<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<button value="A">
		<key key="ENTER" />
	</button>
	<button value="B">
		<key key="BACKSPACE" />
	</button>
	<button value="X">
		<key key="ESC" />
	</button>
	<button value="Y">
		<state id="1" next="2">
			<key key="ALT" />
			<key key="TAB" />
		</state>
		<state id="2">
			<key key="TAB" />
		</state>
	</button>
	<button value="RB">
		<key key="LEFT_MOUSE" />
	</button>
	<button value="LB">
		<key key="RIGHT_MOUSE" />
	</button>
	<button value="START">

	</button>
	<button value="BACK">

	</button>
	<button value="RS">
		<key key="CENTER_MOUSE" />
	</button>
	<button value="LS">
		<key key="CTRL" />
		<key key="SHIFT" />
		<key key="T" />
	</button>
	<pov>
		<N>
			<key key="UP_ARROW" />
		</N>
		<S>
			<key key="DOWN_ARROW" />
		</S>
		<E>
			<key key="RIGHT_ARROW" />
		</E>
		<W>
			<key key="LEFT_ARROW" />
		</W>
		<NE>
			<key key="UP_ARROW" />
			<key key="RIGHT_ARROW" />
		</NE>
		<NW>
			<key key="UP_ARROW" />
			<key key="LEFT_ARROW" />
		</NW>
		<SE>
			<key key="DOWN_ARROW" />
			<key key="RIGHT_ARROW" />
		</SE>
		<SW>
			<key key="DOWN_ARROW" />
			<key key="LEFT_ARROW" />
		</SW>
	</pov>
</configuration>